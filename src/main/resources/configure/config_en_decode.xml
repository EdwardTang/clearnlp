<configuration>
    <reader type="column">
        <column index="1"  field="id"/>
        <column index="2"  field="form"/>
        <column index="4"  field="lemma"/>
        <column index="6"  field="pos"/>
        <column index="7"  field="feats"/>
        <column index="9"  field="headId"/>
        <column index="11" field="deprel"/>
        <column index="13" field="sheads"/>
    </reader>
    
    <models>
        <language>en</language>
        <dictionary>model/dictionary-1.2.0.zip</dictionary>
        <model mode="pos"   path="model/ontonotes-en-pos-1.3.0.tgz"/>
        <model mode="dep"   path="model/ontonotes-en-dep-1.3.0.tgz"/>
        <model mode="morph" path="model/dictionary-1.2.0.zip"/>
        <model mode="pred"  path="model/ontonotes-en-pred-1.3.0.tgz"/>
        <model mode="role"  path="model/ontonotes-en-role-1.3.0.tgz"/>
    </models>

    <pos>
        <algorithm name="adagrad" iter="8" rand="2" alpha="0.01" rho="0.1"/>
        <threads>42</threads>
    </pos>
    
    <dep>
        <algorithm name="adagrad" iter="7,6,7" rand="5" alpha="0.01" rho="0.2"/>
        <threads>42</threads>
    </dep>
    
    <pred>
        <algorithm name="liblinear" solver="0" cost="0.3" eps="0.1" bias="-1"/>
        <threads>42</threads>
    </pred>
    
    <role>
        <algorithm name="liblinear" solver="0" cost="0.2" eps="0.1" bias="-1"/>
        <threads>42</threads>
    </role>

    <srl>
        <algorithm name="adagrad" iter="9,10,9,10,10" rand="2" alpha="0.01" rho="0.2"/>
        <algorithm name="adagrad" iter="9,10,9,10,10" rand="2" alpha="0.01" rho="0.1"/>
        <threads>42</threads>
    </srl>
    
    <srlx>
        <algorithm name="liblinear" solver="0" cost="0.1" eps="0.1" bias="-1"/>
        <algorithm name="liblinear" solver="0" cost="0.1" eps="0.1" bias="0.1"/>
        <threads>42</threads>
    </srlx>
    
    
    
    
    
    
    
    
    
    
    
    <pos-algorithm>
        <algorithm name="adagrad" iter="8"  rand="7"  alpha="0.01" rho="0.2" note="Chinese Treebank 5.1"/>
        <algorithm name="adagrad" iter="6"  rand="11" alpha="0.01" rho="0.1" note="Penn Treebank"/>
        <algorithm name="adagrad" iter="10" rand="11" alpha="0.01" rho="0.1" note="OntoNotes English"/>
    </pos-algorithm>
        
    <pos-conll-ca-cn-cz-es-jp>
        <algorithm name="liblinear" solver="0" cost="0.2" eps="0.1" bias="-1"/>
        <threads>40</threads>
    </pos-conll-ca-cn-cz-es-jp>

    <pos-conll-de>
        <algorithm name="liblinear" solver="0" cost="0.3" eps="0.1" bias="-1"/>
        <threads>40</threads>
    </pos-conll-de>

    <pos-conll-en>
        <algorithm name="liblinear" solver="0" cost="0.2" eps="0.1" bias="0.6"/>
        <algorithm name="adagrad" iter="1" alpha="0.01" delta="0.2"/>
        <threads>40</threads>
    </pos-conll-en>

    <pos-penn>
        <algorithm name="liblinear" solver="0" cost="0.1" eps="0.1" bias="0.7"/>
        <threads>40</threads>
    </pos-penn>
</configuration>



